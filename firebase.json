{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/make_goro",
        "function": "make_goro"
      }, 
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
